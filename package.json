  },
  "devDependencies": {
    "add-text-to-markdown": "2.0.0",
    "commitizen": "4.0.3",
    "cz-emoji": "1.1.2",
    "documentation": "12.1.4",
    "kocha": "1.9.1",
    "semistandard": "14.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji/"
    }
