{
  "name": "slack_group_creator",
  "version": "1.0.0",
  "description": "a bot to create private group",
  "main": "./src/index.js",
  "author": "code4kit",
  "contributors": [
    "shion0625",
    "yeohsoonkeat",
    "waricoma"
  ],
  "engines": {
    "yarn": "1.19.1",
    "node": "13.1"
  },
  "license": "MIT",
  "scripts": {
